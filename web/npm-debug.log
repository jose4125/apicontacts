30322 silly resolved     version: '0.5.2',
30322 silly resolved     description: 'Traverse and transform objects by visiting every node on a recursive walk',
30322 silly resolved     author: { name: 'James Halliday' },
30322 silly resolved     license: 'MIT/X11',
